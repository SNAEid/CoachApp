<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="XamarinFirebaseApp.Views.View.WelcomPage">
    <ContentPage.Content>
        <StackLayout VerticalOptions="CenterAndExpand" Orientation="Vertical">
            <Label Text="Welcome To MyCoach" FontSize="Header" VerticalOptions="CenterAndExpand" HorizontalOptions="Start"/>
            <Button   x:Name="Time" Text="to Time" BackgroundColor="Black" TextColor="White" Clicked="Time_Clicked"/>

            <Button   x:Name="Logout" Text="Logout" BackgroundColor="Black" TextColor="White" Clicked="Logout_Clicked"/>

        </StackLayout>
    </ContentPage.Content>
</ContentPage>